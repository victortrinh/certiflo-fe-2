{
  "env": {
    "es2021": true
  },
  "extends": ["google", "prettier", "prettier/react"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }, { "usePrettierrc": true }],
    "react/prop-types": 0,
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": "off"
  }
}
